<Window x:Class="ThreeDPdfMaker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="578">
    <Grid>
        <Button x:Name="ButtonWebToDesktop" Content="Run web to desktop tests" HorizontalAlignment="Left" Margin="20,223,0,0" VerticalAlignment="Top" Width="144" Click="ButtonButtonWebToDesktop_Click"/>
        <Button x:Name="ButtonDataDir" Content="Choose data directory" HorizontalAlignment="Left" Margin="20,33,0,0" VerticalAlignment="Top" Width="123" Click="ButtonDataDir_Click"/>
        <Button x:Name="ButtonSTLFile" Content="Choose .stl file" HorizontalAlignment="Left" Margin="20,103,0,0" VerticalAlignment="Top" Width="123" Click="ButtonSTLFile_Click" ToolTip="Choose the OrthoVis session file to use"/>
        <Button x:Name="ButtonMakePDF" Content="Make 3D pdf" HorizontalAlignment="Left" Margin="20,174,0,0" VerticalAlignment="Top" Width="144" Click="ButtonMakePDF_Click"/>
        <Label x:Name="LabelSessionFile" Content="No file chosen" HorizontalAlignment="Left" Margin="20,128,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelDataDir" Content="No directory chosen" HorizontalAlignment="Left" Margin="20,58,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelDebug" Content="" HorizontalAlignment="Left" Margin="20,257,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
